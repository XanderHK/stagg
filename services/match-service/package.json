{
  "name": "@stagg/match-service",
  "version": "1.0.0",
  "author": "Parker Ballner",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev src/express.ts",
    "dev:win": "set NODE_ENV=development && ts-node-dev src/express.ts",
    "prod": "tsc && node dist/express.js",
    "build": "tsc",
    "deploy": "yarn build && gcloud functions deploy match-service --trigger-http --entry-point=default --runtime=nodejs12 --region=us-east1 --memory=128MB --timeout=540 --max-instances=128 --env-vars-file=.env.yaml",
    "start": "node dist/index.js",
    "test": "jest"
  },
  "dependencies": {
    "@stagg/db": "^1.0.3",
    "@types/express": "^4.17.9",
    "callofduty": "^1.0.3",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5",
    "express-graphql": "^0.12.0",
    "graphql": "^15.4.0"
  }
}
