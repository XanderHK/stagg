    "test": "exit 0"
  },
  "dependencies": {
    "@stagg/callofduty": "^0.8.2",
    "@stagg/mdb": "^0.8.2",
    "@stagg/util": "^0.8.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.0.13",
    "dotenv": "^8.2.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5"
  }
}
