{
  "name": "@stagg/faas-render-html",
  "version": "0.8.0",
  "author": "Dan Lindsey",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev src/express.ts",
    "prod": "tsc && node dist/express.js",
    "build": "tsc && copyfiles -u 1 src/html/*.html dist/",
    "deploy": "yarn build && gcloud functions deploy render-html --trigger-http --entry-point=default --runtime=nodejs12 --region=us-east1 --memory=512MB",
    "start": "node dist/index.js",
    "test": "ts-jest"
  },
  "dependencies": {
    "node-html-to-image": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.0.13",
    "body-parser": "^1.19.0",
    "copyfiles": "^2.3.0",
    "express": "^4.17.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5"
  }
}
