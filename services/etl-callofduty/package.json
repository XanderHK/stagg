{
  "name": "@stagg/etl-callofduty",
  "version": "1.0.1",
  "author": "Dan Lindsey",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev src/express.ts",
    "dev:win": "set NODE_ENV=development && ts-node-dev src/express.ts",
    "prod": "tsc && node dist/express.js",
    "build": "tsc",
    "deploy": "yarn build && gcloud functions deploy etl-callofduty --trigger-http --entry-point=default --runtime=nodejs12 --region=us-east1 --memory=128MB --timeout=540 --max-instances=128 --env-vars-file=.env.yaml",
    "start": "node dist/index.js",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "callofduty": "^0.9.9",
    "@stagg/db": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.13",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5"
  }
}
